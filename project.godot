config/description="简单的随机地牢"
run/main_scene="res://scenes/start_menu/StartMenu.tscn"
boot_splash/image="res://assets/icon/atri.png"
boot_splash/bg_color=Color( 0.976471, 0.694118, 0.521569, 1 )
config/icon="res://assets/icon/220905.png"

[autoload]

Config="*res://scenes/config.gd"
GlobalState="*res://scripts/GlobalState.gd"
LevelState="*res://scripts/LevelState.gd"
WidgetFactory="*res://scripts/WidgetFactory.gd"
